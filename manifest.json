{
	"name": "Combinations",
	"description": "Shortcut Key Combinations !!",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": [
		"cookies",
		"alarms",
		"scripting",
		"storage",
		"activeTab"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"icons": {
		"16": "/images/bear16.png",
		"32": "/images/bear32.png",
		"48": "/images/bear48.png",
		"128": "/images/bear128.png"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "/images/baby16.png",
			"32": "/images/baby32.png",
			"48": "/images/baby48.png",
			"128": "/images/baby128.png"
		}
	}
}